/*
** Test script file for CScript exponentiation operator.
*/

assert(2**2 == 4);          # (folded)
assert(2**3 == 8);          # (folded)
assert(2**-3 == 0.125);     # (folded)
a = 2**2;                   # (folded)
assert(a == 4);
assert(a**2 == 16);         # (not folded)
assert(2**a == 16);         # (not folded)
