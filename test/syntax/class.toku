local a = {};
assert(typeof(a) == "table");
a.b = class {
    fn name() {
        return "Class";
    }
};
assert(typeof(a.b) == "class");
local t = getmethods(a.b);
foreach k,v in pairs(t)
    print(k,v);
print("+++++");
a = class inherits a.b {
    fn myname() {
        return "Sub" .. super.name();
    }
};
assert(typeof(a) == "class");
a = a();
t = getmethods(a);
foreach k,v in pairs(t)
    print(k,v);
assert(typeof(a) == "instance");
assert(a.name() == "Class");
assert(a.myname() == "SubClass");
